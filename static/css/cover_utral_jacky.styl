.cover
  background-image: url(../assets/cover_bg.jpg);
  background-position: center top;
  background-size: 100% auto;
  background-repeat: no-repeat;
  background-color: #1f162b;
  text-align: center;

.cover .title_10th
  width: 25%;
  position: absolute;
  left: 50%;
  top: 5px;
  margin-left: -12.5%;
  z-index: 99;

.cover .deformation
  width: 60%;
  height: 100%;
  position: relative;
  margin: 0 auto;
  margin-top: 37.4%;

.cover .deformation li
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
  opacity: 0;

.cover .deformation li.deformation01
  -webkit-transition-delay: 0s;
  transition-delay: 0s;
  -webkit-transform: translate(34%, -9.1%);
  -ms-transform: translate(34%, -9.1%);
  transform: translate(34%, -9.1%);

.cover .deformation li.deformation02
  -webkit-transition-delay: 0.1s;
  transition-delay: 0.1s;
  -webkit-transform: translate(-33%, -8.8%);
  -ms-transform: translate(-33%, -8.8%);
  transform: translate(-33%, -8.8%);

.cover .deformation li.deformation03
  -webkit-transition-delay: 0.2s;
  transition-delay: 0.2s;
  -webkit-transform: translate(-33%, 9%);
  -ms-transform: translate(-33%, 9%);
  transform: translate(-33%, 9%);

.cover .deformation li.deformation04
  -webkit-transition-delay: 0.3s;
  transition-delay: 0.3s;
  -webkit-transform: translate(-33%, 8.9%);
  -ms-transform: translate(-33%, 8.9%);
  transform: translate(-33%, 8.9%);

.cover .deformation li.deformation05
  -webkit-transition-delay: 0.4s;
  transition-delay: 0.4s;
  -webkit-transform: translate(33.3%, -9%);
