.ultra .title
  position: absolute;
  width: 90%;
  left: 5%;
  top: 10%;

.ultra .inf
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 10px;
  color: #fff;
  font-weight: bold;
  font-size: 18px;
  text-shadow: -1px -1px 0px rgba(0, 0, 0, 0.61), 1px 1px 0px rgba(0, 0, 0, 0.46), 0px 0px 11px #ffffff;

.ultra .inf span
