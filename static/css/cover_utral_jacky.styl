.jacky
  background-color: #facb16;

.jacky .fire,
.jacky .poster
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;

.jacky .fire
  background-color: #facb16;
  /* Rectangle: */
  background-image: -webkit-linear-gradient(289deg, #facb16 0%, #fcd86b 23%, #ffe7c7 28%, #fad116 35%, #faa216 42%, #fa8c16 49%, #facb16 100%);
  background-image: linear-gradient(161deg, #facb16 0%, #fcd86b 23%, #ffe7c7 28%, #fad116 35%, #faa216 42%, #fa8c16 49%, #facb16 100%);

.jacky .poster
  background-image: url(../assets/jacky_portrait.png);
  background-size: 100% auto;
  background-position: center top;
  background-repeat: no-repeat;
  -webkit-transition-property: -webkit-transform;
  transition-property: transform;
  -webkit-transition-duration: 10s;
  transition-duration: 10s;
  -webkit-transition-timing-function: ease-out;
  transition-timing-function: ease-out;
  -webkit-transition-delay: 0s;
  transition-delay: 0s;
  -webkit-transform: scale(1.1, 1.1);
  -ms-transform: scale(1.1, 1.1);
  transform: scale(1.1, 1.1);

.jacky .title
  position: relative;
  z-index: 99;
  width: 80%;
  margin-top: 62%;
  -webkit-transition-property: -webkit-transform;
  transition-property: transform;
  -webkit-transition-duration: 10s;
  transition-duration: 10s;
  -webkit-transition-timing-function: ease-out;
  transition-timing-function: ease-out;
  -webkit-transition-delay: 0s;
  transition-delay: 0s;
  -webkit-transform: scale(0.9, 0.9);
  -ms-transform: scale(0.9, 0.9);
  transform: scale(0.9, 0.9);

.jacky .company
  position: absolute;
  right: 10px;
  bottom: 10px;
  z-index: 10;
  width: 50%;

.jacky .spark_a,
.jacky .spark_b
  position: absolute;
  right: 16%;
  bottom: 0;
  -webkit-transform-origin: right bottom;
  -ms-transform-origin: right bottom;
  transform-origin: right bottom;
  opacity: 0;
  -webkit-animation-timing-function: cubic-bezier(0.18, 0.3, 0.86, 0.37);
  animation-timing-function: cubic-bezier(0.18, 0.3, 0.86, 0.37);
  -webkit-animation-iteration-count: infinite;
  animation-iteration-count: infinite;

.jacky .spark_a
  width: 40%;
  -webkit-animation-duration: 3s;
  animation-duration: 3s;

.jacky .spark_b
  width: 20%;
  -webkit-animation-duration: 1.4s;
  animation-duration: 1.4s;

.fp-completely.jacky .fire
  -webkit-animation-name: firebg;
  animation-name: firebg;
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-timing-function: ease-in-out;
  animation-timing-function: ease-in-out;
  -webkit-animation-iteration-count: infinite;
  animation-iteration-count: infinite;

.fp-completely.jacky .poster
  -webkit-transform: scale(1, 1);
  -ms-transform: scale(1, 1);
  transform: scale(1, 1);

.fp-completely.jacky .title
  -webkit-transform: scale(1, 1);
  -ms-transform: scale(1, 1);
  transform: scale(1, 1);

.fp-completely.jacky .spark_a,
.fp-completely.jacky .spark_b
  -webkit-animation-name: spark;
  animation-name: spark;

.ultra
  background-color: #030102;

.ultra .ultra_bg
  background-image: url(../assets/ultra_bg.jpg);
  background-repeat: no-repeat;
  background-size: 100% auto;
  background-position: 50% top;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  -webkit-filter: brightness(2);
  filter: brightness(2);
  -webkit-transform: scale(1.1, 1.1);
  -ms-transform: scale(1.1, 1.1);
  transform: scale(1.1, 1.1);
  -webkit-transform-origin: 50% 80%;
  -ms-transform-origin: 50% 80%;
  transform-origin: 50% 80%;
  -webkit-transition-property: -webkit-transform, -webkit-filter, -webkit-transform, filter;
  transition-property: transform, filter;
  -webkit-transition-duration: 2s, 2s;
  transition-duration: 2s, 2s;
  -webkit-transition-timing-function: cubic-bezier(0.47, 0.09, 0.63, 0.99);
  transition-timing-function: cubic-bezier(0.47, 0.09, 0.63, 0.99);
  -webkit-transition-delay: 0s;
  transition-delay: 0s;

.ultra .triangle
