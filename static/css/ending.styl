
.page-ending

  .ending-bg
    left 0
    top 0
    right 0
    position absolute
    bottom 0
    width 100%
    z-index 2

.ending-logo
  z-index 3
  width 60%
  display block
  margin auto
  position absolute
  left 50%
  top 0
  -webkit-transform translate(-50%, 210px)
  -moz-transform translate(-50%, 210px)
  -ms-transform translate(-50%, 210px)
  -o-transform translate(-50%, 210px)
  transform translate(-50%, 210px)
  -webkit-transition all cubic-bezier(0.51, 0.04, 0.5, 0.99) 1.2s prop time
  -moz-transition all cubic-bezier(0.51, 0.04, 0.5, 0.99) 1.2s
  -ms-transition all cubic-bezier(0.51, 0.04, 0.5, 0.99) 1.2s
  -o-transition all cubic-bezier(0.51, 0.04, 0.5, 0.99) 1.2s
  transition all cubic-bezier(0.51, 0.04, 0.5, 0.99) 1.2s

.ending-tips
  position absolute
  bottom 0
  left 0
  right 0
  text-align center
  font-size 9px
  color #ffffff
  z-index 6
  line-height 40px

.ending-contact
  position absolute
  z-index 9
  display: none
  color #fff
  left 50%
  transform translateX(-50%)
  top 210px
  text-align center
  width 80%
  .ending-icon
    width 55%
    display block
    margin-right 8px
    margin-top 8px
  div
    font-size 14px
    padding-top 10px
    padding-bottom 5px
  small
    font-size 60%
    display block
  table
    width 100%
    margin auto
    opacity 0
    -webkit-transition all linear 500ms 500ms
    -moz-transition all linear 500ms 500ms
    -ms-transition all linear 500ms 500ms
    -o-transition all linear 500ms 500ms
    transition all linear 500ms 500ms

    a
     color #fff
     text-decoration none

    td
      padding-top: 8px
      padding-bottom: 8px
    td:nth-of-type(2n)
      font-weight bold
      font-size: 18px
    td:nth-of-type(1)
      font-weight 200
      font-size 10px
    &:nth-last-of-type(1)
      width 65%

&.fp-completely
  table
    opacity 1
    margin-bottom: 1em;
  .ending-logo
    -webkit-transform: translate(-50%, 20px)
    -moz-transform: translate(-50%, 20px)
    -ms-transform: translate(-50%, 20px)
    -o-transform: translate(-50%, 20px)
    transform: translate(-50%, 20px)

